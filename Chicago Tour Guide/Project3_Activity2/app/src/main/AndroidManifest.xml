            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".AttractionsQuoteFragment"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".AttractionsActivity"
            android:exported="false" />
        <activity
